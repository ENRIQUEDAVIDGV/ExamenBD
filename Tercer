/* CREACIÓN DE LA TABLA NUMERO DOS LA CUAL NO TIENE NINGUNA RELACION A LA PRIMERA */
Create table TABLA2 (
Id number (10),
Tipo char (50)
);

Select * from TABLA2
Drop table TABLA2; 

/* INSERTAMOS CON UN NEXVAL/SECUENCIA */
Create or replace trigger nex
before insert
on TABLA2
begin
insert into TABLA2 values (tabla.nextval,'Higiene personal');
end nex;

Drop trigger nex; 

/* CREACIÓN DE LA SECUENCIA */
Create sequence tabla
start with 1
increment by 1 
maxvalue 20
minvalue 1;

Drop sequence tabla;

/* INSERTAMOS LOS VALORES A LA TABLA2 */
insert into TABLA2 values (tabla.nextval,'Comida'); 
insert into TABLA2 values (tabla.nextval,'Frituras'); 
insert into TABLA2 values (tabla.nextval,'Bebidas'); 
insert into TABLA2 values (tabla.nextval,'Reposteria'); 
insert into TABLA2 values (tabla.nextval,'Dulce'); 
insert into TABLA2 values (tabla.nextval,'Pan'); 
insert into TABLA2 values (tabla.nextval,'Alcohol'); 
insert into TABLA2 values (tabla.nextval,'Higiene personal');
insert into TABLA2 values (tabla.nextval,'Limpieza');
insert into TABLA2 values (tabla.nextval,'Perfume');
insert into TABLA2 values (tabla.nextval,'Tabaco');

/* CREACIÓN DE LA TABLA TRES */
Create table TAB3 (
id number (10),
marca nvarchar2 (100)
);

Select * from TAB3;
Drop table TAB3; 

/* INSERTAMOS LOS VALORES A LA TABLA3 */
insert into TAB3 (id, marca) values (1,'Tesoem');
