/* JOINS */

Select Productos.nombre, Productos.precio, TABLA2.Tipo, TAB3.marca from TABLA2 full join Productos  ON TABLA2.Tipo = Productos.Num_tipo full join TAB3 ON TAB3.marca = Productos.Num_mar;

Select * from Productos inner join TAB3 on Num_mar = marca where marca='Tesoem';

Select * from Productos left join TABLA2 on nombre = Tipo;

Select * from Productos right join TAB3 on Num_mar = marca;  

Select * from Productos full join TABLA2 on Num_tipo = Tipo full join TAB3 on Num_mar = marca where Num_tipo is null;

Select * from Productos full join TABLA2 on Num_tipo = tipo full join TAB3 on Num_mar = marca  where nombre like 'T%'; 

/* EXTRA: INSERTAR EN LA TABLA "PRODUCTOS" UN PRODUCTO QUE EMPIECE CON "T" Y MOSTRAS EN LA TABLA "EXT" SE INSERTO UN PRODUCTO CON LA VITAMINA T */
Create table ext (
id number (10),
algo char (100)
);

Select * from ext;
Drop table ext;

create or replace trigger ext1
after insert
on Productos 
for each row
when (new.nombre like 'T%') 
begin
insert into ext values (1,'Se inserto un producto con la vitamina T ,'|| :new.nombre);
end ext1;

drop trigger ext1;

/* INSERTAMOS LOS DATOS */
insert into Productos (id,nombre, precio, Num_tipo, Num_mar) values (76,'Tostadas', 20, 'Comida', 'Tesoem');
insert into Productos (id,nombre, precio, Num_tipo, Num_mar) values (77,'Tamal', 20, 'Comida', 'Tesoem');
insert into Productos (id,nombre, precio, Num_tipo, Num_mar) values (78,'Toptops', 20, 'Fritura', 'Sabritas');

Select * from Productos;
Select * from ext; 

/* CREACION DE LA VISTA QUE NOS MUESTRA EL ID, NOMBRE,PRECIO, TIPO Y MARCA, EN ESTA MISMA SOLO SE VISUALIZARAN PRODUCTOS CON UN PRECIO MAYOR A 25 PERO MENOR O IGUAL A 35 */
Create or replace view Vista1
as
Select id, nombre, precio, Num_tipo, Num_mar
from Productos where precio>25 and id<=35 ;

Select * from Vista1;
Drop view Vista1; 

/* SAQUEME UN 100, YA TENGO SUEÑO Y DE TANTA COMIDA ME DIO HAMBRE */
