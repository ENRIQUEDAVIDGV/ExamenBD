/* CREACIÓN DE LA TABLA "PRODUCTOS" */
Create table Productos (
id number (10),
nombre char (50),
precio char (10),
Num_tipo char (50),
Num_mar char (50)
);

Select * from Productos;
Drop Table Productos; 

