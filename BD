SISTEMAS COMPUTACIONALES:

Create table prof_ISC(
id number (3),
Num_empleado number (11) ,
Nombre varchar2 (60),
Apellidos varchar2 (60),
Carrera varchar2 (30),
Nombre_clase varchar  (60),
grupo varchar (9)
);
 
drop table prof_ISC

create or replace procedure profes_ISC
as
begin
insert into  prof_ISC (id, Num_empleado, Nombre,Apellidos, Carrera, Nombre_clase,grupo)/* se insertaran en la tabla creada de sistemas los campos que se seleccionaron de la tabla principal */
select id, Num_empleado, Nombre, Apell_pa | | ' ' | |  Apell_ma, Carrera, Nombre_clase,grupo  from profe_tesoem  /* se van a seleccionar los campos que se mandaran a traer de la tabla principial de profesores, concatenando unicamente apellido materno y apellido paterno*/
where Carrera = 'ISC';/*en donde unicamente llamara a los profesores que den alguna materia en la carrera "ISC" que son todos los de sistemas */
end;

Execute profes_ISC
Select * from prof_ISC

TICS:

create table Prof_TICS(
id number (3)  unique,
Num_empleado number (9)not null ,
Nombre varchar2 (60),
Apellidos varchar2 (60),
Carrera varchar2 (30),
Nombre_clase varchar  (60),
grupo varchar (5)
);

create or replace procedure profes_TICS
as
begin
insert into Prof_TICS (id, Num_empleado, Nombre,Apellidos, Carrera, Nombre_clase,grupo)
select id, Num_empleado, Nombre,  Apell_pa | | ' ' | |  Apell_ma, Carrera, Nombre_clase,grupo  from profe_tesoem 
where Carrera = 'TICS';
end;

Execute profes_TICS
Select* from Prof_TICS

